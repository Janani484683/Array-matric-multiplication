#include<stdio.h>
int main() {
    int A[2][2];
    int r,c;
    printf(" enter row and column:");
    scanf ("%d%d",&r,&c);
    printf("matrix A");
    for(int i =0; i<r; i++) {
        for(int j=0; j<c; j++) {
            scanf("%d",&A[i][j]);
        }
    }
    for(int i=0; i<r; i++) {
        for(int j=0; j<c; j++) {
            printf("%d ",A[i][j]);
        }
        printf("\n");
    }
    return 0;
    }
